    "bugs": {
        "url": "https://github.com/brumi1024/yarn-scheduler-ui/issues"
    },
    "homepage": "https://github.com/brumi1024/yarn-scheduler-ui#readme",
    "scripts": {
        "start": "vite --port 8080",
        "dev": "vite --port 8080",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "lint": "eslint .",
        "lint:ci": "eslint . --ext .js,.jsx,.ts,.tsx,.html --format @microsoft/eslint-formatter-sarif --output-file eslint-results.sarif",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"**/*.{js,html,css,md,json,ts,tsx}\" --ignore-path .gitignore",
        "test": "vitest",
        "test:ui": "vitest --ui"
