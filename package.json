{
    "name": "yarn-scheduler-ui",
    "version": "0.1.0",
    "type": "module",
    "description": "A modern web-based interface for managing Apache Hadoop YARN Capacity Scheduler configurations. This tool provides an intuitive visual interface for viewing, editing, and managing queue hierarchies in YARN clusters.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/brumi1024/yarn-scheduler-ui.git"
    },
    "keywords": [],
    "author": "",
